{
  "private": true,
  "name": "frontend-challenge",
  "author": "Adrian C. Miranda <adriancmiranda@gmail.com>",
  "homepage": "http://adriancmiranda.github.io/",
  "main": "server/app.js",
  "scripts": {
    "check": "npm-check -E",
    "precommit": "redrun -s lint test",
    "commitmsg": "commitlint -x @commitlint/config-conventional -e $GIT_PARAMS",
    "docs": "rimraf docs && esdoc",
    "lint": "echo 'The lint script task is not available yet'",
    "test": "redrun -s test:*",
    "test:coverage": "echo 'The test:coverage script task is not available yet'",
    "test:unit": "echo 'The test:unit script task is not available yet'",
    "test:e2e": "echo 'The test:e2e script task is not available yet'",
    "dev": "cross-env HOST=localhost PORT=3000 NODE_ENV=development nodemon --ignore 'public/manifest.assets.json' server",
    "dev:public": "cross-env PORT=3000 NODE_ENV=development nodemon --ignore 'public/manifest.assets.json' server",
    "dev:tunnel": "redrun -P dev:public ngrok",
    "build": "webpack-cli --env=production --progress --profile --colors --verbose --config=server/webpack/config.prod.js",
    "ngrok": "ngrok http 9000 -host-header=localhost:9000",
    "docker": "cross-env PORT=80 npm start",
    "start": "node server"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "colors": "1.4.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "ip": "1.1.5",
    "morgan": "1.9.1",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "babel-loader": "8.0.6",
    "cross-env": "7.0.0",
    "esdoc": "1.1.0",
    "nodemon": "2.0.2",
    "redrun": "7.1.17",
    "rimraf": "3.0.2",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-manifest-plugin": "2.2.0",
    "whatwg-fetch": "3.0.0"
  }
}
